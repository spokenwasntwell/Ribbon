{
  "name": "discord-ribbon",
  "version": "8.0.0-beta1",
  "description": "feature rich, modular Discord.JS-Commando server bot",
  "author": "Favna <sharkie.jeroen@gmail.com> (https://favna.xyz/home)",
  "license": "GPL-3.0",
  "main": "src/app.ts",
  "homepage": "https://favna.xyz/ribbon",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/favna/ribbon.git"
  },
  "bugs": {
    "url": "https://github.com/favna/ribbon/issues"
  },
  "scripts": {
    "docs": "jsdoc2md --template ./docs/template.hbs --files ./src/commands/**/*.ts | tee docs/index.md ../Ribbon.wiki/All-Commands.md > NUL",
    "docs:json": "jsdoc2md --json --files ./src/commands/**/*.ts > ../favna.xyz/public/dist/ribbondocs.json",
    "docs:deploy": "yarn run docs && yarn run docs:json && git commit -am documentation && git push && cd ../Ribbon.wiki/ && git commit -am documentation && git push && cd ../favna.xyz/ && git commit -am documentation && git push",
    "lint": "eslint src/commands/**/*.ts src/*.ts src/components/*.ts",
    "lint:fix": "eslint --fix src/commands/**/*.ts src/*.ts src/components/*.ts",
    "start": "ts-node src/app.ts bow",
    "start:ribbon": "ts-node src/app.ts",
    "start:debug": "ts-node --inspect src/app.ts bow",
    "test": "yarn lint && mocha"
  },
  "keywords": [
    "bot",
    "Commando",
    "discord",
    "discord.js",
    "discord.js-commando",
    "Favna",
    "NodeJS",
    "TypeScript"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
