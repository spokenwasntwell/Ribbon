  },
  "bugs": {
    "url": "https://github.com/favna/ribbon/issues"
  },
  "scripts": {
    "docs": "jsdoc2md --template ./docs/template.hbs --files ./src/commands/**/*.ts | tee docs/index.md ../Ribbon.wiki/All-Commands.md > NUL",
    "docs:json": "jsdoc2md --json --files ./src/commands/**/*.ts > ../favna.xyz/public/dist/ribbondocs.json",
    "docs:deploy": "yarn run docs && yarn run docs:json && git commit -am documentation && git push && cd ../Ribbon.wiki/ && git commit -am documentation && git push && cd ../favna.xyz/ && git commit -am documentation && git push",
    "lint": "tslint -p . -c tslint.json src/commands/**/*.ts src/components/*.ts src/*.ts",
    "lint:fix": "yarn lint --fix",
    "start": "ts-node src/app.ts bow",
    "start:ribbon": "ts-node src/app.ts",
    "test": "yarn lint && mocha"
  },
  "keywords": [
    "bot",
    "Commando",
    "discord",
    "discord.js",
    "discord.js-commando",
    "Favna",
    "NodeJS",
    "TypeScript"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
